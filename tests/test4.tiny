read x;
if x > 0 then
  fact := 1;
  repeat
    fact := fact * x;
    x := x - 1
  until x = 0;
  write fact
end;
read y;
if y < 10 then
  sum := 0;
  repeat
    sum := sum + y;
    y := y + 1
  until y = 10;
  write sum
else
  write y
end;
read a;
read b;
if a = b then
  write a
else
  if a < b then
    write b
  else
    write a
  end
end
